@article{haraEffectArmSwing2006,
  title = {The Effect of Arm Swing on Lower Extremities in Vertical Jumping},
  author = {Hara, Mikiko and Shibayama, Akira and Takeshita, Daisuke and Fukashiro, Senshi},
  date = {2006},
  journaltitle = {Journal of Biomechanics},
  shortjournal = {Journal of Biomechanics},
