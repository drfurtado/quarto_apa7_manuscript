@article{kambasRelationshipMotorProficiency2012,
  title = {The Relationship between Motor Proficiency and Pedometer-Determined Physical Activity in Young Children},
  author = {Kambas, Antonis and Michalopoulou, Maria and Fatouros, Ioannis G. and Christoforidis, Christos and Manthou, Eirini and Giannakidou, Dimitra and Venetsanou, Fotini and Haberer, Elke and Chatzinikolaou, Athanassios and Gourgoulis, Vassilios and Zimmer, Renate},
