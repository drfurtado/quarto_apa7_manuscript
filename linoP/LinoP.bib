@article{barnett2009,
  title = {Childhood {{Motor Skill Proficiency}} as a {{Predictor}} of {{Adolescent Physical Activity}}},
  author = {Barnett, Lisa M and family=Beurden, given=Eric, prefix=van, useprefix=true and Morgan, Philip J. and Brooks, Lyndon O. and Beard, John R.},
  date = {2009},
  journaltitle = {Journal of Adolescent Health},
